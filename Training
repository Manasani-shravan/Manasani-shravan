# list concept practicing
class10 = ["shravan","shetty","vamshi" ,"rakesh" ,"suresh" ,"ganga" ,"durga"]
print(class10[1:4])
print(class10[0:7])
print(class10[1:3])
print(class10[6:])
print(class10[-7:-1])
print(class10[:2])
print(class10[7:])
print(class10[:6])
print(class10[1:])
print(class10[3])
print(class10[:6])
print(class10[0:])
